# Server Configuration
PORT=3001
NODE_ENV=production

# API Authentication
API_KEY=your-secret-api-key-here

# CORS - Allowed origins (comma-separated)
ALLOWED_ORIGINS=https://your-luna-app.vercel.app,http://localhost:3000

# Sessions Path (where WhatsApp sessions are stored)
SESSIONS_PATH=./sessions

# Webhook URL (Luna API endpoint to receive events)
WEBHOOK_URL=https://your-luna-app.vercel.app/api/evo-whatsapp/webhook
